    if mytype = 3
    {
        draw_set_font(global.messagefont)
        draw_set_alpha(0.5)
        draw_rectangle_colour(x,y,x+272,y+80,0,0,0,0,0)
        draw_set_alpha(1)
        draw_text(x,y,string_make_width(content,280))
    }
    //Draw text and destination modifiers
    else
    {
        draw_text(x,y,content)
    }
    
