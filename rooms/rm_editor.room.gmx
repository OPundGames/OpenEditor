  <code>//Open level editor if in edit mode
if global.editorstate = es_edit
{
    instance_create(0,0,obj_editor)
}
//Otherwise, play the level
else
{
    //Create BG Animator object
    instance_create(0,0,obj_bganimator)
    
    //Create level manager
    instance_create(0,0,obj_levelmanager)
    
    //If Mario is dead..
    if global.died = 1
    {
        //Spawn Mario at checkpoint if he collected it
        if global.checkpointsection != noone
        {
            global.currentsection = global.checkpointsection
            instance_create(global.checkpointx+8,global.checkpointy+16,obj_mario)
        }
        
        //Otherwise, send him back to the beginning
        else
        {
            global.currentsection = "MAIN"
        }
        
    }
    
    //Bring Mario back from the dead
    global.died = 0
    
    //Generate level from data
    scr_gen_level_play(global.currentsection)
    
    //Create Mario at top left corner if he's not already created
    if !instance_exists(obj_mario)
    and !instance_exists(obj_warpmario)
    and !instance_exists(obj_jumpspawn)
    and !instance_exists(obj_vinespawn)
    {
        instance_create(0,0,obj_mario)
    }
}</code>
